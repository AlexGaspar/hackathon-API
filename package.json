{
  "name": "node-boilerplate",
  "version": "0.0.1",
  "description": "Boilplate node application.",
  "main": "./lib",
  "dependencies": {
    "bunyan": "~0.22.0",
    "config": "~0.4.32"
  },
  "devDependencies": {
    "mocha": "~1.14.0",
    "chai": "~1.8.1",
    "jshint": "~2.3.0",
    "jshint-stylish": "~0.1.3"
  },
  "scripts": {
    "start": "node ./lib | ./node_modules/.bin/bunyan",
    "test": "npm run lint && npm run mocha",
